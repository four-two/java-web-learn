<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <!--名称-->
        <servlet-name>oneServlet</servlet-name>
        <!--接口实现类路径-->
        <servlet-class>controller.OneServlet</servlet-class>
    </servlet>
    <servlet>
        <!--名称-->
        <servlet-name>twoServlet</servlet-name>
        <!--接口实现类路径-->
        <servlet-class>controller.TwoServlet</servlet-class>
        <!--服务器启动时创建 默认值为0-->
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>threeServlet</servlet-name>
        <servlet-class>controller.ThreeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fourServlet</servlet-name>
        <servlet-class>controller.FourServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fiveServlet</servlet-name>
        <servlet-class>controller.FiveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SixServlet</servlet-name>
        <servlet-class>controller.SixServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SevenServlet</servlet-name>
        <servlet-class>controller.SevenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SevenServlet</servlet-name>
        <url-pattern>/SevenServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SixServlet</servlet-name>
        <url-pattern>/SixServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>oneServlet</servlet-name>
        <!--访问别名-->
        <url-pattern>/oneServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>twoServlet</servlet-name>
        <url-pattern>/twoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>threeServlet</servlet-name>
        <url-pattern>/threeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fourServlet</servlet-name>
        <url-pattern>/fourServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fiveServlet</servlet-name>
        <url-pattern>/fiveServlet</url-pattern>
    </servlet-mapping>
</web-app>